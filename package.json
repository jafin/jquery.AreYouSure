{
  "name": "jquery.AreYouSure",
  "description": "A light-weight jQuery \"dirty forms\" Plugin - it monitors HTML forms and alerts users to unsaved changes if they attempt to close the browser or navigate away from the page. (Are you sure?)",
  "homepage": "https://github.com/codedance/jquery.AreYouSure",
  "author": "Chris Dance <chris.dance@papercut.com> (https://github.com/codedance)",
  "type": "commonjs",
  "contributors": [
    "Tom Clift <tom.clift@papercut.com> (https://github.com/tclift)",
    "Jon Egerton <jon@ja2.co.uk> (http://www.jonegerton.com/)",
    "Scadoodles (https://github.com/Scadoodles)",
    "Albin Sunnanbo (https://github.com/albinsunnanbo)",
    "Marc Sutton <ashre@iname.com> (http://www.codev.co.uk)"
  ],
  "version": "1.9.1",
  "license": "MIT/GPLv2",
  "keywords": [
    "dirty",
    "form",
    "onbeforeunload",
    "save",
    "check"
  ],
  "main": "jquery.are-you-sure.js",
  "engines": {
    "node": ">=0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codedance/jquery.AreYouSure"
  },
  "bugs": {
    "url": "https://github.com/codedance/jquery.AreYouSure/issues"
  },
  "dependencies": {
    "jquery": ">=3.6.0"
  },
  "devDependencies": {
    "@types/jasmine": "^4.3.1",
    "@types/jquery": "^3.5.16",
    "@types/karma": "^6.3.3",
    "babel-core": "~6.26.3",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-preset-env": "~1.7.0",
    "cpx": "~1.5.0",
    "jasmine-core": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^0.1.3",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^0.1.5",
    "karma-jasmine": "^5.1.0",
    "karma-typescript": "^5.5.3",
    "rimraf": "~2.6.2",
    "rollup": "^3.14.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5",
    "uglify-js": "^3.17.4"
  },
  "scripts": {
    "postinstall": "node_modules/.bin/bower install",
    "clean": "rimraf dist && rimraf coverage",
    "build2": "npm run compile && npm run copy:html && npm run bundle && npm run minify",
    "build": "tsup",
    "compile": "tsc",
    "copy:html": "cpx src/**.html dist",
    "test": "karma start",
    "bundle": "rollup -c",
    "minify": "uglifyjs dist/example-plugin-bundle.js --output dist/example-plugin-bundle.min.js --source-map \"filename='dist/example-plugin-bundle.min.js.map',url='example-plugin-bundle.min.js.map',content='dist/example-plugin-bundle.js.map'\""
  }
}
